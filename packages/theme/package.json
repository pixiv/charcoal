{
  "name": "@charcoal-ui/theme",
  "version": "5.0.0-beta.7",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./unstable-token-object": {
      "types": "./dist/unstable-token-object/index.d.ts",
      "require": "./dist/unstable-token-object/index.cjs",
      "import": "./dist/unstable-token-object/index.js",
      "default": "./dist/unstable-token-object/index.js"
    },
    "./unstable-css/*": "./dist/unstable-css/*",
    "./unstable-tokens/*": "./dist/unstable-tokens/*"
  },
  "typesVersions": {
    "*": {
      "unstable-token-object": [
        "./dist/token-object/index.d.ts"
      ],
      "unstable-tokens/*": [
        "./dist/unstable-tokens/*"
      ],
      "unstable-css/*": [
        "./dist/unstable-css/*"
      ]
    }
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "tsdown --config-loader unconfig",
    "typecheck": "tsc --pretty --noEmit",
    "clean": "rimraf dist",
    "test": "vitest run --passWithNoTests",
    "bench": "vitest bench --passWithNoTests"
  },
  "devDependencies": {
    "@types/css": "^0.0.38",
    "css": "^3.0.0",
    "polished": "^4.1.4"
  },
  "dependencies": {
    "@charcoal-ui/foundation": "workspace:*",
    "@charcoal-ui/utils": "workspace:*",
    "change-case-all": "^2.1.0",
    "deepmerge": "^4.3.1"
  },
  "files": [
    "src",
    "dist",
    "theme.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pixiv/charcoal.git",
    "directory": "packages/theme"
  },
  "gitHead": "e1ece2e43901ae667afdd5c178040607d939dcd5"
}
