{
  "name": "@charcoal-ui/icons",
  "version": "5.3.0-beta.1",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts",
        "default": "./dist/index.d.ts"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./css/*": "./css/*",
    "./react/*": "./react/*",
    "./*": "./dist/*"
  },
  "types": "./dist/index.d.ts",
  "sideEffects": true,
  "scripts": {
    "build": "run-p --print-label 'build:*'",
    "build:bundle": "tsdown",
    "build:react": "tsc --project tsconfig.react.json --pretty",
    "typecheck": "tsc --pretty --noEmit",
    "clean": "rimraf dist .tsbuildinfo .tsbuildinfo.react",
    "test": "vitest run --passWithNoTests"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/styled-components": "^5.1.21",
    "@types/warning": "^3.0.0",
    "@types/webpack-env": "^1.18.8",
    "@vitejs/plugin-react": "^4.3.1",
    "jsdom": "^24.1.0",
    "react": "^18.3.1",
    "styled-components": "^5.3.11"
  },
  "dependencies": {
    "@charcoal-ui/icon-files": "workspace:*",
    "warning": "^4.0.3"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "files": [
    "src",
    "dist",
    "svg",
    "css",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pixiv/charcoal.git",
    "directory": "packages/icons"
  },
  "gitHead": "e1ece2e43901ae667afdd5c178040607d939dcd5"
}
